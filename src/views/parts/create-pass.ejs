<section>
  <h2>Crie sua própria sala</h2>
  <form action="/create-room" method="POST">
    <label class="sr-only" for="room-id">Criar sua própria sala</label>
    <input
      type="password"
      name="password"
      id="room-id"
      placeholder="Insira uma senha"
      autofocus="autofocus"
      onfocus="this.select()"
    />
    <button>
      <img src="images/users-white.svg" alt="Entrar na sala" />
      Criar sala
    </button>
  </form>
</section>
